{
  "permissions": {
    "allow": [
      "read",
      "write",
      "edit",
      "execute",
      "shell"
    ],
    "deny": [
      "delete"
    ]
  },
  "settings": {
    "auto_install_dependencies": true,
    "allowed_commands": [
      "git",
      "sed",
      "awk",
      "grep",
      "find",
      "cat",
      "ls",
      "python",
      "python3",
      "pip",
      "pip3",
      "pytest",
      "black",
      "flake8",
      "mypy",
      "poetry",
      "virtualenv",
      "source"
    ]
  },
  "paths": {
    "allowed": [
      ".",
      "./",
      "/tmp"
    ],
    "denied": [
      "..",
      "../",
      "~",
      "/home",
      "/Users"
    ]
  }
}